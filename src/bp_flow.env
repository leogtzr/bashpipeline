# bashpipeline sequential flow.
# Specific project env variables:

# Project name:
export PROJECT_NAME="Data Feed"
# Type of execution
# SEQUENTIAL or FILE DEFINED
export FLOW_TYPE=SEQ
# Working directory
export WORKING_DIR=feed

# Log debug flag:
export DEBUG=1
export DEBUG_FILE=logs/log_$(date '+%Y%m%S_%H%M%S').log
export INCLUDE_DATE_LOG=1
